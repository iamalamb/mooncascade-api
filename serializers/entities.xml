<?xml version="1.0" ?>
<serializer xmlns="http://symfony.com/schema/dic/serializer-mapping"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://symfony.com/schema/dic/serializer-mapping
        http://symfony.com/schema/dic/serializer-mapping/serializer-mapping-1.0.xsd"
>
    <class name="Mooncascade\Entities\Athlete">
        <attribute name="id">
            <group>event_overview</group>
        </attribute>
        <attribute name="code">
            <group>event_overview</group>
        </attribute>
        <attribute name="name">
            <group>event_overview</group>
        </attribute>
        <attribute name="startNumber">
            <group>event_overview</group>
        </attribute>
        <attribute name="timeAtGate">
            <group>event_overview</group>
        </attribute>
        <attribute name="timeAtFinish">
            <group>event_overview</group>
        </attribute>
    </class>
    <class name="Mooncascade\Entities\Gender">
        <attribute name="name">
            <group>event_overview</group>
        </attribute>
    </class>
    <class name="Mooncascade\Entities\Team">
        <attribute name="name">
            <group>event_overview</group>
        </attribute>
    </class>
</serializer>